<sdf version="1.6">
  <model name="{{cookiecutter.structure_name}}">
    <static>true</static>
   
    <link name="link">
      <collision name="collision">
        <geometry>
          <box>
            <size>1 1 1</size>
          </box>
        </geometry>
      </collision>

      <visual name= "structure">
        <geometry>
          <mesh>
           <uri>model://{{cookiecutter.structure_name}}/meshes/Structure.dae</uri>
          </mesh>
        </geometry>
      </visual> 
    </link>
  </model>
</sdf>
